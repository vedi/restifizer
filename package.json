{
  "name": "restifizer",
  "version": "0.8.21",
  "description": "Restful database-agnostic service",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-migrate && npm run test-cucumber ; npm run test-migrate-undo",
    "test-cucumber": "istanbul cover cucumber.js ./test",
    "test-migrate": "cd ./test/lib/app/ && sequelize db:migrate",
    "test-migrate-undo": "cd ./test/lib/app/ && sequelize db:migrate:undo && echo 'migration:undo completed'"
  },
